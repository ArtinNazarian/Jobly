{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "firstName",
    "lastName",
    "email",
    "phone number",
    "City",
    "State"
  ],
  "properties": {
    "firstName": {
      "type": "string",
      "default": "",
      "title": "The firstName Schema",
      "examples": ["John"]
    },
    "lastName": {
      "type": "string",
      "default": "",
      "title": "The lastName Schema",
      "examples": ["Smith"]
    },
    "email": {
      "type": "string",
      "default": "",
      "title": "The email Schema",
      "examples": ["john_smith@gmail.com"]
    },
    "phone number": {
      "type": "string",
      "default": "",
      "title": "The phone number Schema",
      "examples": ["505-410-1234"]
    },
    "City": {
      "type": "string",
      "default": "",
      "title": "The City Schema",
      "examples": ["Echo Park"]
    },
    "State": {
      "type": "string",
      "default": "",
      "title": "The State Schema",
      "examples": ["CA"]
    }
  },
  "examples": [
    {
      "firstName": "John",
      "lastName": "Smith",
      "email": "john_smith@gmail.com",
      "phone number": "505-410-1234",
      "City": "Echo Park",
      "State": "CA"
    }
  ]
}
